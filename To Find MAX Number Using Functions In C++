#include <iostream.h>
#include <conio.h>

int max(int num1, int num2);

int main () 
{
int a ;
int b;
int x;
clrscr();

cout << "Enter 1st number: ";
cin >> a;
cout << "Enter 2nd number: ";
cin >> b;

x = max(a, b);
cout << "Max value is : " << x << endl;

getch();
